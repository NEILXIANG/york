<div id="myCarousel" class="carousel slide">
<ol class="carousel-indicators">
	<li data-target="#myCarousel" data-slide-to="0" class="active"></li>
	<li data-target="#myCarousel" data-slide-to="1"></li>
	<li data-target="#myCarousel" data-slide-to="2"></li>
</ol>
<!-- Carousel items -->
<div class="carousel-inner">
	<div class="active item"><img src="/assets/1.jpg"></div>
	<div class="item"><img src="/assets/2.jpg"></div>
	<div class="item"><img src="/assets/6.jpg"></div>
</div>
<!-- Carousel nav -->
<a class="carousel-control left" href="#myCarousel" data-slide="prev">&lsaquo;</a>
<a class="carousel-control right" href="#myCarousel" data-slide="next">&rsaquo;</a>
</div>
<script>
	$('.carousel').carousel({
		  interval: 5000
		})
</script>
<div class="well">
	<h5 style="text-align: center">关注最多</h2>
</div>
<%= render partial: 'products/liked_list', locals: {products: @products} %>
<div class="well">
	<h5 style="text-align: center">访问最多</h2>
</div>
<%= render partial: 'product_users/visit_list', locals: {products: @products} %>

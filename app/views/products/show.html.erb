#encoding:utf-8
<div class="span12">
	<%= @product.title %><br />
	<% @product.photos.each do |photo| %>
		<%= image_tag(photo.image_url) %>
	<% end %> <br />
	<%= @product.number %><br />
	<%= @product.use %>
	<%= @product.load %><br />
	<%= @product.content %><br />
	<%= link_to t('.back'), products_path, class: "btn btn-primary" %> 
	<%= link_to t('.edit'), edit_product_path(@product),class: "btn btn-info" %>
